{
  "expo": {
    "name": "Gamaze ScoreMaster",
    "slug": "gamez-scoreboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gamaze_logo.png",
    "scheme": "monsgameapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/gamaze_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.gamaze.scoremaster",
      "icon": "./assets/images/gamaze_logo.png",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/gamaze_logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "963e62e4-45d4-4041-b1bf-7c039489cf15"
      }
    }
  }
}
